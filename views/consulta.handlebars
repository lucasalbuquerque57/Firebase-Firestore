    <body>
        <div class="container">
            <h1>Consulta</h1>
            <table class="table">
                <thead>
                    <tr>
                        <th>Nome</th>
                        <th>Telefone</th>
                        <th>Origem</th>
                        <th>Data de Contato</th>
                        <th>Ações</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each posts}}
                    {{#dataValues}}
                    <tr>
                        <td>{{nome}}</td>
                        <td>{{telefone}}</td>
                        <td>{{origem}}</td>
                        <td>{{data_contato}}</td>
                        <td>{{cidade}}</td>
                        <td>{{estado}}</td>
                        <td>
                            <a href="/editar/{{id}}"><button class="btn btn-primary" h>Editar</button></a>
                            <a href="/excluir/{{id}}"><button class="btn btn-danger">Excluir</button></a>
                        </td>
                    </tr>
                    {{/dataValues}}
                    {{/each}}
                </tbody>
            </table>
        </div>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
